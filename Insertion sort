void insert(int a[],int b[])
{
    int i,j,temp;
    for(i=1,i<n;i++)
{
   temp=a[i];
    j=i-1;
    while(i>=0&&temp<=a[j])
    {
    a[j+1]=a[j];
    j=j-1;
 }
   a[j+1]=temp;
  }
}
 void print after(inta[],int n)   
{
    int i;
    for i=0;i<n;i++);
    printf("%d",a[i]);
}
